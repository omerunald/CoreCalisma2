@model Abc.UI.Model.ShippingDetailsViewModel

@{
    Layout = "_Layout";
}
<form asp-controller="Cart" asp-action="Complete" method="post">
    <div class="form-group">
        <label asp-for="ShippingDetails.FirstName"></label>
        <input asp-for="ShippingDetails.FirstName" class="form-control col-md-6" placeholder="" />
        <span asp-validation-for="ShippingDetails.FirstName"></span>
    </div>
    <div class="form-group">
        <label asp-for="ShippingDetails.LastName"></label>
        <input asp-for="ShippingDetails.LastName" class="form-control col-md-6" placeholder="" />
        <span asp-validation-for="ShippingDetails.LastName"></span>
    </div>
    <div class="form-group">
        <label asp-for="ShippingDetails.Email"></label>
        <input asp-for="ShippingDetails.Email" class="form-control col-md-6" placeholder="" />
        <span asp-validation-for="ShippingDetails.Email"></span>
    </div>
    <div class="form-group">
        <label asp-for="ShippingDetails.City"></label>
        <input asp-for="ShippingDetails.City" class="form-control col-md-6" placeholder="" />
        <span asp-validation-for="ShippingDetails.City"></span>
    </div>
    <div class="form-group">
        <label asp-for="ShippingDetails.Adres"></label>
        <input asp-for="ShippingDetails.Adres" class="form-control col-md-6" placeholder="" />
        <span asp-validation-for="ShippingDetails.Adres"></span>
    </div>
    <div class="form-group">
        <label asp-for="ShippingDetails.Age"></label>
        <input asp-for="ShippingDetails.Age" class="form-control col-md-6" placeholder="" />
        <span asp-validation-for="ShippingDetails.Age"></span>
    </div>
    <input type="submit" value="Alıverişi Tamamla" class="btn btn-success"/>
</form>